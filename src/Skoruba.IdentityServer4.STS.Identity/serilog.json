{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "c:\\temp\\Logs\\identity-server.txt",
          "rollingInterval": "Day",
          "buffered": false
        }
      }
    ]
  }
}